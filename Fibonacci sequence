#include <stdio.h>
int main()
{
    int n,i,a=0,b=1,c=0;
    printf("Enter the number of terms: ");
    scanf("%d",&n);
    printf("Fibonacci sequence up to %d term(s):\n",n);
    for(i=0;i<=n-1;i++)
    {
        printf("%d",a);
        c=a;
        a+=b;
        b=c;
        if(i!=n-1) printf(",");
        else printf(".");
    }
    return(0);
}
